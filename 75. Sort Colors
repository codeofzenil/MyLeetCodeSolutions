nums = [2,0,2,1,1,0]
count0 = 0
count1 = 0
count2 = 0

for val in nums:
    if val == 0:
        count0+=1
    elif val == 1:
        count1+=1
    else:
        count2+=1
        
ans = []
for i in range(count0):
    ans.append(0)
for i in range(count1):
    ans.append(1)
for i in range(count2):
    ans.append(2)    
        
print(ans)
